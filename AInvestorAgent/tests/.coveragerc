[run]
source = backend
omit =
    */tests/*
    */migrations/*
    */__init__.py
    */venv/*

[report]
exclude_lines =
    pragma: no cover
    def __repr__
    raise AssertionError
    raise NotImplementedError
    if __name__ == .__main__.:
    if TYPE_CHECKING:
    @abstractmethod

[html]
directory = tests/reports/coverage_html
